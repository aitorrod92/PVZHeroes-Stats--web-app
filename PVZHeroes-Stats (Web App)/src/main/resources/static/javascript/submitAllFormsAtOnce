$('#form1').submit(doubleSubmit);

function doubleSubmit(e1) {
	console.log("POR AQUÍ");
    e1.preventDefault();
    e1.stopPropagation();

    var post_form1 = $.post($(this).action, $(this).serialize());

    post_form1.done(function(result) {
            $('#form2').submit();
        });
        
        // CONTINUAR CON LOS SIGUIENTES
};